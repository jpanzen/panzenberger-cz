{
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "tailwindcss": "^3.4.4"
  },
  "scripts": {
    "build": "eleventy",
    "serve": "eleventy --serve",
    "tailwind": "npx tailwindcss -i ./src/input.css -o ./src/output.css --watch",
    "dev": "npx tailwindcss -i ./src/input.css -o ./src/output.css --watch && npx @11ty/eleventy --serve"
  }
}
